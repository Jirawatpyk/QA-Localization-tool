# Supabase — Project settings > API
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Supabase — Project settings > API > Service Role (server-only, NEVER expose to client)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Supabase — Database connection (pooler URL, port 6543)
DATABASE_URL=postgresql://postgres:password@db.your-project.supabase.co:6543/postgres

# Inngest — Dashboard > Manage > Keys
INNGEST_EVENT_KEY=your-inngest-event-key
INNGEST_SIGNING_KEY=your-inngest-signing-key

# OpenAI — L2 AI provider (gpt-4o-mini)
OPENAI_API_KEY=sk-your-openai-key

# Anthropic — L3 AI provider (claude-sonnet-4-5-20250929)
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key

# Upstash Redis — Rate limiting
UPSTASH_REDIS_REST_URL=https://your-redis.upstash.io
UPSTASH_REDIS_REST_TOKEN=your-upstash-token

# Google OAuth (configure in Supabase Dashboard > Authentication > Providers)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Supabase Webhook Secret (for auth webhooks, optional)
SUPABASE_WEBHOOK_SECRET=your-webhook-secret

# E2E Testing (Playwright)
E2E_TEST_PASSWORD=TestPassword123!
